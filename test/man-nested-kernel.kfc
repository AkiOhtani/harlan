;; -*- scheme -*-

;; An example from the Harlan manual. We want to make sure these all
;; run.

(module
  (extern print_2x2_int_vec ((vec (vec int 2) 2)) -> void)
  
  (define (main)
    (print_2x2_int_vec
     (kernel ((a (vector 2 3)))
       (kernel ((b (vector 5 7)))
         (* a b))))
    (return 0)))

