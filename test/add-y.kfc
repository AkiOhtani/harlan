;; -*- scheme -*-

;; This is like simple-kernel.kfc, but it uses a free variable instead.

(module
  (define (main)
      (let Y (vector 1 2 3 4))
      (let y 1)
      (let X (kernel ((x (var Y)))
                     (+ (var y) (var x))))
      (assert (= (vector-ref X 0) 2))
      (assert (= (vector-ref X 1) 3))
      (assert (= (vector-ref X 2) 4))
      (assert (= (vector-ref X 3) 5))
      (return 0)))
