;; -*- scheme -*-

;; A test case making sure vectors are somewhat working.

(module
  (define (main)
      (let v (vector 1 2 3 4))
      (let len (length v))
      (print len)
      (assert (= 4 len))
      (print (vector-ref v 0))
      (print (vector-ref v 1))
      (print (vector-ref v 2))
      (print (vector-ref v 3))
      (assert (= 1 (vector-ref v 0)))
      (assert (= 2 (vector-ref v 1)))
      (assert (= 3 (vector-ref v 2)))
      (assert (= 4 (vector-ref v 3)))
      (return 0)))
